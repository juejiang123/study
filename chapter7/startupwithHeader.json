[
  {
    "description": "这是个带header的请求",
    "request": {
      "uri": "/post/with/header",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
      "json": {
        "name": "wangli",
        "sex": "female"
      }
    },
    "response": {
      "json": {
        "wangli": "success",
        "status": "1"
      }
    }
  }
]